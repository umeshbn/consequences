class Jungle{
			static void aniAll(String animals[]){
				
				for(int app=0;app<animals.length;app=app+1){
					System.out.println(animals[app]);
				}
			}
			  public static void main(String[] args){
			    String[] animals={"wildboar","deer","tiger","lion","panther"};
				aniAll(animals);
			  System.out.println();
				animals[0]="human";
				aniAll(animals);
				animals[4]="somepeople";

			  
			  }
	}